{
  "name": "react-joi-validation",
  "version": "1.5.0",
  "description": "Extremely flexible validation using Joi and React",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "rm -rf lib && babel --presets=es2015,stage-0,react src -d lib",
    "build-for-tests": "babel --presets=es2015,stage-0,react src -d lib --source-maps",
    "build-tests": "rm -rf lib/spec && babel --presets=es2015,stage-0,react spec -d lib/spec --source-maps",
    "tests": "npm run build-for-tests && npm run build-tests && jasmine",
    "watch-tests": "nodemon --exec 'npm run tests || true'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greena13/react-joi-validation.git"
  },
  "keywords": [
    "React",
    "Joi",
    "validation",
    "forms"
  ],
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "author": "Aleck Greenham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/greena13/react-joi-validation/issues"
  },
  "homepage": "https://github.com/greena13/react-joi-validation#readme",
  "dependencies": {
    "invariant": "^2.2.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.drop": "^4.1.1",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.keys": "^4.2.0",
    "lodash.map": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.topath": "^4.5.2",
    "lodash.uniq": "^4.5.0",
    "lodash.unset": "^4.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.0.0",
